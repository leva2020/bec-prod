<div class="row clearfix">
    {% if reports %}
        {% for report in reports %}
            <div name="{{ index0.index }}" class="col-md-4 column">
                <div>
                    <p>
                        <a data-id="3952" href="{{ path('entity.node.canonical', {'node': report.id}) }}"
                           title="{{ report.title.value }}">
                            {% set imagestyle = {
                                '#theme':      'image_style',
                                '#style_name': 'custom_360x171',
                                '#uri':        report.field_image.entity.uri.value,
                                '#alt':        '',
                            } %}
                            {{ imagestyle }}
                        </a>
                    </p>
                </div>
            </div>
            {% if index0.index < 1 %}
                <div name="{{ index0.index }}" class="col-md-4 column">
                    <div>
                        <h5 style="text-align: center;">
                            <a data-id="3816"
                               href="/prime/usuarios-registrados/"
                               title="Usuarios Registrados">
                                <br><br><br>&lt;
                                Inicio
                            </a>
                        </h5>
                    </div>
                </div>
            {% endif %}
        {% endfor %}
    {% endif %}
</div>