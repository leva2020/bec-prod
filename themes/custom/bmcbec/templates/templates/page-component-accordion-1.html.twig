{% if data.entity.field_titulo.value %}
<div class="page_component accordion">
    <div class="panel-group" id="accordion-{{item_test}}" role="tablist" aria-multiselectable="true">
            {% for item in data %}
        <div class="panel panel-default">
              <div class="panel-heading" role="tab" id="heading-{{loop.index}}-{{item_test}}">
                  <p class="heading panel-title">
                      <a data-toggle="collapse" data-parent="#accordion-{{item_test}}" href="#collapse-{{loop.index}}-{{item_test}}" aria-expanded="false" aria-controls="collapse-{{loop.index}}-{{item_test}}" class="collapsed">
                        {{item.entity.field_titulo.value}}
                          <i class="fa fa-chevron-down pull-right after highlight"></i>
                      </a>
                  </p>
              </div>
              <div id="collapse-{{loop.index}}-{{item_test}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="collapse-{{loop.index}}-{{item_test}}" aria-expanded="false" style="height: 0px;">
                  <div class="panel-body">
                    {% for item1 in item.entity.field_archivos %}
                        <p>
                            <a href="{{ file_url(item1.entity.field_archivo.entity.fileuri) }}">hola</a>
                            <a href="{{ file_url(item1.entity.field_archivo[0].entity.fileuri) }}" target="_blank">
                                {{item1.entity.field_label_archivo.value}}
                            </a>
                        </p>
                    {% endfor %}
                  </div>
              </div>
        </div>
            {% endfor %}
    </div>
</div>
{% endif %}
