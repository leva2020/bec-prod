<section class="content advanced-component tabbed-listing color-bg-main-gray">
		<div class="container">

			<!-- ROW - OVERVIEW -->
			<div class="row overview os-animation animated fadeIn" data-os-animation="fadeIn" data-os-animation-delay="0s">
				<div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 col-xs-12 text-center">
					<h2 class="heading main">{{ data.field_titulo.value }}</h2>
					<div class="text ">
						<p><span class="intro">{{ data.field_descripcion_html.value|raw }}</span></p>
					</div>
				</div>
			</div>
			<!--// ROW - OVERVIEW -->

			<!-- ROW -->
			<div class="row">
				<div class="tabbed col-xs-12" role="tabpanel">
					<ul class="nav nav-tabs responsive text-center hidden-xs hidden-sm" role="tablist">
						<li role="presentation" class="active">
							<a href="#tab-name-2252-1" aria-controls="tab-name-2252-1" role="tab" data-toggle="tab">
								<i class="fa fa-chevron-down pull-right after highlight"></i>
							</a>
						</li>
					</ul>

					<div class="tab-content responsive hidden-xs hidden-sm">
						<div id="tab-name-2252-1" role="tabpanel" class="tab-pane active">
							<!-- LISTING -->
							<div class="row listing section grid">
                {% for item in data.field_cuadros_listado %}
								<div class="item col-md-4 col-sm-6 col-xs-12">
									<a href="{{ item.entity.field_link.url }}" title="{{ item.entity.field_link.title }}">
										<div class="image">
                      <img src="{{file_url(item.entity.field_imagen.entity.fileuri)}}" title="{{item.entity.field_imagen.entity.title}}" alt="{{item.entity.field_imagen.entity.alt}}">
										</div>
										<p class="heading">{{ item.entity.field_titulo.value }}</p>
                    {{item.entity.field_descripcion_1.value|raw}}
									</a>
							</div>
                {% endfor %}
								<!--// ITEM -->
							</div>
							<!--// LISTING  -->
							<p class="link text-center"><a class="btn"></a></p><!--Elemento fijo-->
						</div>
					</div>
				</div>
			</div>
			<!--// ROW -->

		</div>
	</section>
